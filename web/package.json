{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test_headless": "ng test --watch false --browsers ChromeHeadless",
    "webdriver-start": "webdriver-manager start &",
    "webdriver-update": "webdriver-manager update",
    "e2e_headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "heroku-postbuild": "ng build --configuration=production",
    "stats": "webpack-bundle-analyzer dist/iplusplus/stats.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run web:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
